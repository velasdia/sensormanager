<%= semantic_form_for @sensor do |f| %>
  <%= f.inputs do %>
    <%= f.input :Concentrador %>
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :estado %>
    <%= f.input :parametro %>
    <%= f.input :valor %>
    <%= f.input :uds %>
    <%= f.input :gis_latitud %>
    <%= f.input :gis_longitud %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
