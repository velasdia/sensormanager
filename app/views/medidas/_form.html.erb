<%= semantic_form_for @medida do |f| %>
  <%= f.inputs do %>
    <%= f.input :Sensor, :collection => Sensor.all.collect{|cat|[cat.nombre]} %> 
    <%= f.input :Sensor %>
    <%= f.input :fecha %>
    <%= f.input :valor %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
